{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "commonjs": true
    },
    "rules": {
        "linebreak-style":["warn", "windows"],
        "no-unused-vars":["error",{
            "vars":"local",
            "args":"none"
        }],
        "indent":["off", "4"]
    }
}